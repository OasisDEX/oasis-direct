{
  "name": "oasis-direct",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "^4.7.3",
    "@ledgerhq/hw-transport-u2f": "^4.7.3",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "bignumber.js": "^7.2.1",
    "bluebird": "^3.5.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "create-react-class": "^15.6.2",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "hdkey": "^0.8.0",
    "html-webpack-plugin": "2.29.0",
    "jazzicon": "^1.5.0",
    "jest": "20.0.4",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.3.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.3.1",
    "react-tooltip": "^3.6.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "web3": "^0.20.6",
    "web3-provider-engine": "14.0.4",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "OASIS_GANACHE_COMPATIBILITY=1 node scripts/start.js",
    "build": "rm -rf ./build && REACT_APP_ENV='prod' node scripts/build.js",
    "build:test": "OASIS_GANACHE_COMPATIBILITY=1 REACT_APP_ENV='test' node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "deploy:stage": "npm run build && surge ./build/ https://stage-oasis-direct.surge.sh",
    "surge": "npm run build && surge ./build/ https://oasis-direct.surge.sh",
    "cypress:run:ci": "cypress run --record",
    "cypress:run": "cypress run",
    "cypress:dev": "cypress open",
    "metamask-e2e:dev": "NODE_ENV=test DEV=1 ETH_PROVIDER=http://localhost:8545 mocha --timeout 30000 --require babel-register ./metamask-e2e/tests.js",
    "metamask-e2e:run": "NODE_ENV=test ETH_PROVIDER=http://localhost:8545 mocha --timeout 30000 --require babel-register --exit ./metamask-e2e/tests.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.2.0",
    "cypress": "^3.1.0",
    "http-server": "^0.11.1",
    "metamask-puppeteer": "^0.0.3",
    "mocha": "^5.2.0",
    "puppeteer": "^1.10.0",
    "puppeteer-better-utils": "^0.0.3",
    "surge": "^0.20.1",
    "truffle-privatekey-provider": "^0.1.0"
  }
}
